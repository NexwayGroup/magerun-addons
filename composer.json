{
    "name": "nexway/magerun-addons",
    "type": "magento-module",
    "version": "1.0.0",
    "description": "Magerun addons: SetupManeger",
    "homepage": "http://nexway.com",
    "license": "MIT",
    "require": {
        "php": ">=5.4.0",
        "n98/magerun": "*",
        "magento-hackathon/magento-composer-installer": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "Mockery/Mockery": ">=0.7.2"
    },
    "extra": {
        "magento-root-dir": "root"
    },
    "autoload": {
        "psr-4": {
            "Nexway\\": "src/Nexway",
            "Tests\\": "tests/"
        }
    }
}
